{
  "name": "matias-gulp-cli",
  "version": "1.0.1",
  "description": "gulp-cli方便项目快速集成gulp到工程中",
  "main": "gulpfile.js",
  "bin": {
    "gulp-cli": "./bin/init.js"
  },
  "scripts": {
    "publish-prerelease": "gulp versionPrerelease && gulp gitVersionPush",
    "publish-patch": "gulp versionPatch && gulp gitVersionPush",
    "publish-minor": "gulp versionMinor && gulp gitVersionPush",
    "publish-major": "gulp versionMajor && gulp gitVersionPush",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matiastang/gulp-cli.git"
  },
  "author": "matias",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matiastang/gulp-cli/issues"
  },
  "homepage": "https://github.com/matiastang/gulp-cli#readme",
  "dependencies": {
    "commander": "^8.3.0",
    "del": "^6.0.0",
    "download-git-repo": "^3.0.2",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-json-editor": "^2.5.6",
    "gulp-load-plugins": "^2.0.7",
    "inquirer": "^8.2.0",
    "minimist": "^1.2.5",
    "shelljs": "^0.8.4"
  }
}
